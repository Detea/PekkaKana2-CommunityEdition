<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="12.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|Win32'">
    <LocalDebuggerWorkingDirectory>$(SolutionDir)..\bin\</LocalDebuggerWorkingDirectory>
    <DebuggerFlavor>WindowsLocalDebugger</DebuggerFlavor>
    <LocalDebuggerCommand>$(SolutionDir)..\bin\$(ProjectName).exe</LocalDebuggerCommand>
    <LocalDebuggerCommandArguments>dev debug</LocalDebuggerCommandArguments>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|Win32'">
    <LocalDebuggerWorkingDirectory>$(SolutionDir)..\bin\</LocalDebuggerWorkingDirectory>
    <DebuggerFlavor>WindowsLocalDebugger</DebuggerFlavor>
    <LocalDebuggerCommand>$(SolutionDir)..\bin\$(ProjectName)_debug.exe</LocalDebuggerCommand>
    <LocalDebuggerCommandArguments>dev debug windowed nosound</LocalDebuggerCommandArguments>
    <LocalDebuggerDebuggerType>Auto</LocalDebuggerDebuggerType>
  </PropertyGroup>
</Project>